{
    "presets": [
        "@babel/preset-react",
        "@babel/preset-env",
        "@babel/preset-flow"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties",
            {
                "loose": true
            }
        ],
        ["@babel/plugin-transform-flow-strip-types", {"all":  true}]
    ]
}
